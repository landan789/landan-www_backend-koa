    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.39",
    "@types/koa-static": "^4.0.1",
    "@types/sequelize": "^4.27.36",
    "graphql": "^14.1.1",
    "graphql-server-koa": "^1.4.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
